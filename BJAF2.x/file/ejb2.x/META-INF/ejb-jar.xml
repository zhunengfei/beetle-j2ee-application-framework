<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd" version="2.1">
  <display-name>BusinessEJB21</display-name>
  <enterprise-beans>
    <session>
      <ejb-name>CommandServerBean</ejb-name>
      <home>com.beetle.framework.business.command.imp.ejb.CommandServerHome</home>
      <remote>com.beetle.framework.business.command.imp.ejb.CommandServer</remote>
      <local-home>com.beetle.framework.business.command.imp.ejb.CommandServerLocalHome</local-home>
      <local>com.beetle.framework.business.command.imp.ejb.CommandServerLocal</local>
      <ejb-class>com.beetle.framework.business.command.imp.ejb.CommandServerBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Bean</transaction-type>
    </session>
    <session>
      <ejb-name>BusinessDelegateBean</ejb-name>
      <home>com.beetle.framework.business.delegate.imp.ejb.BusinessDelegateHome</home>
      <remote>com.beetle.framework.business.delegate.imp.ejb.BusinessDelegate</remote>
      <local-home>com.beetle.framework.business.delegate.imp.ejb.BusinessDelegateLocalHome</local-home>
      <local>com.beetle.framework.business.delegate.imp.ejb.BusinessDelegateLocal</local>
      <ejb-class>com.beetle.framework.business.delegate.imp.ejb.BusinessDelegateBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Local</method-intf>
        <method-name>executeBusiness</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.delegate.BsRequest</method-param>
        </method-params>
      </method>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>executeBusiness</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.delegate.BsRequest</method-param>
        </method-params>
      </method>
      <trans-attribute>Never</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Local</method-intf>
        <method-name>runServiceSupports</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>runServiceSupports</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <trans-attribute>Supports</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Local</method-intf>
        <method-name>runServiceRequired</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>runServiceRequired</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Local</method-intf>
        <method-name>runServiceNotSupported</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>runServiceNotSupported</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.service.Input</method-param>
        </method-params>
      </method>
      <trans-attribute>NotSupported</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Local</method-intf>
        <method-name>executeBusinessWithTransaction</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.delegate.BsRequest</method-param>
        </method-params>
      </method>
      <method>
        <ejb-name>BusinessDelegateBean</ejb-name>
        <method-intf>Remote</method-intf>
        <method-name>executeBusinessWithTransaction</method-name>
        <method-params>
          <method-param>com.beetle.framework.business.delegate.BsRequest</method-param>
        </method-params>
      </method>
      <trans-attribute>RequiresNew</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>

